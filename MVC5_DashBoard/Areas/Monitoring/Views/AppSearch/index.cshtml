<script type="text/ng-template" id="AppSearchTemplate.html">
    <div>
        <div class="modal-header">
            <button type="button" class="close" ng-click="cancel()" data-dismiss="modal" aria-hidden="true">&times;</button>
            <h4 class="modal-title">Seach Application</h4>
        </div>
        <div class="modal-body">
            <span for="inputappFilter" class="control-label">Application</span><br />
            <input ng-model="filterApp" id="inputappFilter" type="text" class="form-control"
                   placeholder="Filter by" autofocus>
            <table class="table table-striped">
                <tr ng-repeat="Appli in AppliList | orderBy:'+' | filter:filterApp" ng-click="ok(Appli)"><td class="btn">{{Appli}}<td></td></tr>
            </table>
        </div>
        <div class="modal-footer">
            <button class="btn btn-warning" type="button" ng-click="cancel()">Cancel</button>
        </div>
    </div>
</script>